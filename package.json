{
  "name": "light-bootstrap-dashboard-react",
  "version": "1.2.0",
  "private": true,
  "dependencies": {
    "bootstrap": "3.3.7",
    "chartist": "0.10.1",
    "node-sass": "4.6.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "4.1.2",
    "react": "16.2.0",
    "react-bootstrap": "0.32.1",
    "react-chartist": "0.13.1",
    "react-dom": "16.2.0",
    "react-google-maps": "9.4.5",
    "react-notification-system": "0.2.17",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.1.1",
    "node-notifier": "^5.4.0",
    "reg-keygen-git-hash-plugin": "^0.7.16",
    "reg-notify-github-plugin": "^0.7.19",
    "reg-publish-s3-plugin": "^0.7.18",
    "reg-notify-slack-plugin": "^0.7.16",
    "reg-suit": "^0.7.17",
    "chai": "^4.0.2",
    "chai-webdriverio": "^0.4.1",
    "wdio-chromedriver-service": "^0.1.3",
    "wdio-concise-reporter": "^0.1.2",
    "wdio-mocha-framework": "^0.5.10",
    "wdio-selenium-standalone-service": "0.0.11",
    "wdio-slack-reporter": "^1.0.0",
    "wdio-spec-reporter": "^0.1.5",
    "wdio-static-server-service": "^1.0.1",
    "wdio-visual-regression-service": "^0.8.0",
    "webdriverio": "^4.14.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "eject": "react-scripts eject",
    "test": "wdio",
    "e2e": "npm test -- wdio.conf.dev.js",
    "generateReportDashboard": "reg-cli ./screenshots/latest/ ./screenshots/baseline/ ./screenshots/diff/ -R ./reportBDR.html"
  }
}
